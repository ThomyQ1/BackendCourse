<div class="fixP">
  {{#each products}}
    <div class="card" style="width: 18rem;">
      <img src="{{this.photo}}" class="card-img-top" />
      <div class="card-body">
        <h5 class="card-title">{{this.title}}</h5>
        <p class="card-text">{{this.price}}</p>
        <a href="#" class="btn btn-primary">Ver Producto</a>
      </div>
    </div>
  {{/each}}
</div>